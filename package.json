{
  "name": "careen",
  "version": "0.0.0",
  "description": "SQL schema migration. Nothing more. Nothing less.",
  "main": "index.js",
  "scripts": {
    "build": "tsc @tsc.opts lib/*.ts lib/client/*.ts",
    "build-test": "tsc @tsc.opts declaration/test.d.ts test/*.ts test/client/*.ts",
    "mocha": "mocha",
    "test": "npm run build && npm run build-test && npm run mocha",
    "istanbul": "istanbul cover node_modules/.bin/_mocha",
    "coverage": "npm run build && npm run build-test && npm run istanbul",
    "coveralls": "coveralls < coverage/lcov.info",
    "clean": "find lib test -name '*.js' -delete && rm -rf coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/programble/careen.git"
  },
  "keywords": [
    "sql",
    "schema",
    "migration"
  ],
  "author": "Curtis McEnroe <curtis@cmcenroe.me> (http://cmcenroe.me)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/programble/careen/issues"
  },
  "homepage": "https://github.com/programble/careen",
  "dependencies": {
    "bluebird": "^2.9.14",
    "hemp": "^0.1.2",
    "ramda": "^0.13.0",
    "super-error": "^1.1.2"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.13",
    "mocha": "^2.2.1",
    "mock-fs": "^2.7.0",
    "pg": "^4.3.0",
    "sinon": "^1.14.1",
    "sqlite3": "^3.0.5",
    "typescript": "^1.4.1"
  }
}
